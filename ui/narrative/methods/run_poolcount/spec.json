{
    "ver": "0.0.1",
    "authors": [
        "omreegaloz"
    ],
    "contact": "help@kbase.us",
    "categories": ["active"],
    "widgets": {
        "input": null,
        "output": null
    },
    "parameters": [ 
        {
            "id": "poolfile_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBasePoolTSV.PoolFile"]
            }
        },
        {
            "id": "fastq_files",
            "optional": false,
            "advanced": false,
            "allow_multiple": true,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseFile.SingleEndLibrary" ]
        }
        },
        {
            "id": "genome_ref",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "valid_ws_types": ["KBaseGenomes.Genome"]
            }
        },
        {
        "id": "KB_PoolCount_Bool",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["yes"],
            "field_type" : "dropdown",
            "dropdown_options":{
                "options": [{
                    "value": "no",
                    "display": "No"
                },
                {
                    "value": "yes",
                    "display": "Yes"
                }
                ]
                   }
        },
                {
            "id": "poolcount_description",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "textarea",
            "textarea_options" : {
                "n_rows" : 3

            }
        },

        {
            "id": "output_name",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [ "" ],
            "field_type": "text",
            "text_options": {
                "is_output_name": true
            }
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "poolcount",
            "method": "run_poolcount",
            "input_mapping": [
                {
                    "narrative_system_variable": "workspace",
                    "target_property": "workspace_name"
                },{
                    "narrative_system_variable": "workspace_id",
                    "target_property": "workspace_id"
                },{
                    "input_parameter": "poolfile_ref",
                    "target_property": "poolfile_ref",
                    "target_type_transform": "resolved-ref"

                },{
                    "input_parameter": "fastq_files",
                    "target_property": "fastq_files",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter": "genome_ref",
                    "target_property": "genome_ref",
                    "target_type_transform": "resolved-ref"
                },{
                    "input_parameter": "KB_PoolCount_Bool",
                    "target_property": "KB_PoolCount_Bool"
                },{
                    "input_parameter": "poolcount_description",
                    "target_property": "poolcount_description"
                },
                {
                    "input_parameter": "output_name",
                    "target_property": "output_name"
                }
            ],
            "output_mapping": [
                {
                    "service_method_output_path": [0,"report_name"],
                    "target_property": "report_name"
                },{
                    "service_method_output_path": [0,"report_ref"],
                    "target_property": "report_ref"
                }
            ]
        }
    },
    "job_id_output_field": "docker"
}
